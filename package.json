{
	"name": "alphabetize",
	"displayName": "Alphabetize",
	"description": "Sort lines alphabetically in Visual Studio Code.",
	"version": "0.1.0",
	"publisher": "castanhocorreia",
	"repository": {},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:alphabetize.alphabetize"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "alphabetize.alphabetize",
				"title": "Alphabetize"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"keywords": [
		"alphabetical",
		"extension",
		"sorter",
		"visual-studio-code",
		"vscode"
	],
	"homepage": "",
	"bugs": {},
	"license": "GPL-3.0",
	"author": "Castanho Correia <castanhocorreia.dev@gmail.com>",
	"dependencies": {},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^5.1.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"prettier": "^2.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
